{
  "private": true,
  "name": "cilveku-skaitisana",
  "description": "Electron App",
  "version": "1.0.0",
  "author": "",
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "postinstall": "electron-builder install-app-deps",
    "build": "nextron build",
    "build:all": "nextron build --all",
    "build:win32": "nextron build --win --ia32",
    "build:win64": "nextron build --win --x64",
    "build:mac": "nextron build --mac --x64",
    "build:linux": "nextron build --linux",
    "python-build": "shx rm -rf pythondist && pyinstaller python/app.py --distpath pythondist --workpath build-py-temp --onefile && shx rm -rf build-py-temp && shx rm api.spec"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "electron-context-menu": "^2.5.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^7.0.2",
    "next-fonts": "^1.5.1",
    "next-plugin-svgr": "^1.0.0",
    "python-shell": "^2.0.3",
    "remote": "^0.2.6",
    "sass": "^1.32.8",
    "shx": "^0.3.3"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "next": "^10.0.7",
    "nextron": "^6.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.2.2"
  }
}
